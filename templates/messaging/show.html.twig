{% extends 'base.html.twig' %}

{% block title %}Messaging{% endblock %}

{% block body %}
    <h1>Messaging</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ messaging.id }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ messaging.state ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Archives</th>
                <td>{{ messaging.archives ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Object</th>
                <td>{{ messaging.object }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ messaging.message }}</td>
            </tr>
            <tr>
                <th>MessageDate</th>
                <td>{{ messaging.messageDate ? messaging.messageDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('messaging_index') }}">back to list</a>

    <a href="{{ path('messaging_edit', {'id': messaging.id}) }}">edit</a>

    {{ include('messaging/_delete_form.html.twig') }}
{% endblock %}
